{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "**/*.html": {
      "includeFiles": "**/*.{js,css,webp,mp4,pdf}",
      "excludeFiles": "**/node_modules/**"
    }
  }
}
